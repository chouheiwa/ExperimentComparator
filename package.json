{
  "name": "experiment-comparator",
  "version": "1.0.4",
  "description": "A modern desktop application for comparing machine learning model experiment results",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:build:debug": "tauri build --debug",
    "tauri:build:fast": "tauri build --bundles none",
    "build:win": "tauri build --target x86_64-pc-windows-msvc",
    "build:mac": "tauri build --target universal-apple-darwin",
    "build:linux": "tauri build --target x86_64-unknown-linux-gnu",
    "version:patch": "node scripts/update-version.js patch",
    "version:minor": "node scripts/update-version.js minor", 
    "version:major": "node scripts/update-version.js major",
    "update:deps": "npm update && cd src-tauri && cargo update",
    "package": "npm run build && npm run tauri:build",
    "package:debug": "npm run build && npm run tauri:build:debug",
    "package:all": "npm run build && npm run build:win && npm run build:mac && npm run build:linux"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-dialog": "^2.3.2",
    "@tauri-apps/plugin-fs": "^2.4.1",
    "@types/react-router-dom": "^5.3.3",
    "antd": "^5.26.4",
    "immer": "^10.1.1",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.7.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.7.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
